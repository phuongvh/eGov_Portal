		<field name="endDate" type="Date" />
		<field name="status" type="long" />
	</model>
	<model name="com.vportal.portlet.vcalendar.model.VFile">
		<field name="fileId" type="long" />
		<field name="fullname" type="String">
			<hint name="max-length">200</hint>
