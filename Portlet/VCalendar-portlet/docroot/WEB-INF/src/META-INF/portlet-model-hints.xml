<?xml version="1.0"?>

<model-hints>
	<hint-collection name="TEXTAREA">
		<hint name="max-length">500</hint>
	</hint-collection>
	<model name="com.vportal.portlet.vcalendar.model.VCal">
		<field name="calId" type="long" />
		<field name="groupId" type="long" />
		<field name="companyId" type="long" />
		<field name="userId" type="long" />
		<field name="userName" type="String" />
		<field name="createDate" type="Date" />
		<field name="modifiedDate" type="Date" />
		<field name="languageId" type="String" />
		<field name="fileId" type="long" />
		<field name="title" type="String">
			<hint name="max-length">550</hint>
		</field>
		<field name="description" type="String">
			<hint name="max-length">550</hint>
		</field>
		<field name="guest" type="String">
			<hint name="max-length">200</hint>
		</field>
		<field name="location" type="String">
			<hint name="max-length">200</hint>
		</field>
